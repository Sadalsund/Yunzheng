<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ydsy.mapper.AnnouncementMapper">

    <insert id="insertAnnouncement" parameterType="com.ydsy.pojo.Announcement">
        INSERT INTO announcement (content, direction, period)
        VALUES (#{content}, #{direction}, #{period})
    </insert>

</mapper>
